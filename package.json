{
  "name": "react-money-component",
  "version": "1.4.2",
  "description": "React.js Money Component",
  "main": "lib/money.js",
  "scripts": {
    "test": "jest",
    "lib": "./node_modules/.bin/gulp",
    "build": "npm run lib"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageOnlyFrom": {
      "src/money.js": true
    },
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  },
  "author": "Music Glue Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/musicglue/react-money-component.git"
  },
  "bugs": {
    "url": "https://github.com/musicglue/react-money-component/issues"
  },
  "homepage": "https://github.com/musicglue/react-money-component",
  "keywords": [
    "money",
    "react"
  ],
  "peerDependencies": {
    "accounting": "^0.4.1",
    "@musicglue/js-money": "^0.6.0",
    "react": "^15.0.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-jest": "^5.3.0",
    "coveralls": "^2.11.4",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.3.0",
    "istanbul": "^0.4.0",
    "istanbul-coveralls": "^1.0.3",
    "jest-cli": "^0.6.0",
    "react-addons-test-utils": "^0.14.0",
    "react-dom": "^0.14.0"
  }
}
